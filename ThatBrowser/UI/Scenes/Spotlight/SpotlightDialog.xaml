<ContentDialog
    x:Class="ThatBrowser.UI.Scenes.Spotlight.SpotlightDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    Style="{StaticResource DefaultContentDialogStyle}">
    <ContentDialog.Resources>
        <x:Double x:Key="ContentDialogMaxWidth">10000</x:Double>
        <x:Double x:Key="ContentDialogMaxHeight">10000</x:Double>
        <x:Double x:Key="ContentDialogMinHeight">0</x:Double>
        <Thickness x:Key="ContentDialogPadding">0</Thickness>
        <Thickness x:Key="ContentDialogBorderWidth">0,0,0,0</Thickness>
    </ContentDialog.Resources>
    <Grid>
        <AutoSuggestBox 
            Width="600"
            TextBoxStyle="{ThemeResource SubtitleTextBlockStyle}" 
            PlaceholderText="Search or input url..."/>
    </Grid>
</ContentDialog>
